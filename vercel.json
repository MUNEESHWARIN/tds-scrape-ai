{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/health",
      "dest": "/app.py"
    },
    {
      "src": "/",
      "dest": "/app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "/var/task"
  }
}